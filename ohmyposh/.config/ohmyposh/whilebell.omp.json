{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#1f1f28",
    "foreground": "#dcd7ba",
    "muted": "#727169",
    "accent": "#957fb8",
    "subtle": "#76946a",
    "yellow": "#dca561",
    "blue": "#7e9cd8",
    "aqua": "#6a9589",
    "green": "#98bb6c",
    "orange": "#ffa066",
    "red": "#e46876",
    "purple": "#957fb8"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:muted",
          "style": "plain",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true
          },
          "template": " ({{ if .Venv }}{{ .Venv }}{{ end }})",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:muted",
          "style": "plain",
          "template": " {{.Icon}}",
          "type": "os"
        },
        {
          "foreground": "p:foreground",
          "style": "plain",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:accent",
          "properties": {
            "folder_icon": "\ue5fe ",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:subtle",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:accent",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
