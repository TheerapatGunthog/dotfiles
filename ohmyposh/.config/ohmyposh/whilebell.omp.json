{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#fdf6e3",
    "foreground": "#073642",
    "muted": "#93a1a1",
    "accent": "#268bd2",
    "subtle": "#b58900",
    "yellow": "#b58900",
    "blue": "#268bd2",
    "aqua": "#2aa198",
    "green": "#859900",
    "orange": "#cb4b16",
    "red": "#dc322f",
    "purple": "#6c71c4"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:muted",
          "style": "plain",
          "template": " {{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:foreground",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:accent",
          "properties": {
            "folder_icon": "\ue5fe",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:subtle",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "p:muted",
          "style": "plain",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true
          },
          "template": "({{ if .Venv }}{{ .Venv }}{{ end }}) ",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "foreground": "p:accent",
          "template": " \uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
